{
  "version": "2.02",
  "name": "p4wn",
  "description": "p4wn - chess w/ offline feature",
  "launch_path": "/index-mobile-1.html",
  "chrome": { "navigation": true },
  "icons": {
    "16": "/images/icons/p4wn-16.png",
    "32": "/images/icons/p4wn-32.png",
    "48": "/images/icons/p4wn-48.png",
    "60": "/images/icons/p4wn-60.png",
    "64": "/images/icons/p4wn-64.png",
    "90": "/images/icons/p4wn-90.png",
    "120": "/images/icons/p4wn-120.png",
    "128": "/images/icons/p4wn-128.png",
    "256": "/images/icons/p4wn-256.png"
  },
  "developer": {
    "name": "Oliver Merkel",
    "url": "https://github.com/OMerkel/p4wn"
  },
  "installs_allowed_from": ["*"],
    "permissions": {
        "audio-capture" : {
            "description" : "Audio capture"
        },
        "speech-recognition" : {
            "description" : "Speech recognition"
        }
    }
}
